{
    "files.associations": {
        "*.css": "tailwindcss"
    },
    "editor.quickSuggestions": {
        "strings": "on"
    },
    "tailwindCSS.includeLanguages": {
        "plaintext": "html",
        "javascript": "javascript",
        "javascriptreact": "javascript",
        "typescript": "typescript",
        "typescriptreact": "typescript"
    },
    "editor.inlineSuggest.enabled": true,
    "css.validate": false,
    "less.validate": false,
    "scss.validate": false
}